<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bitcamp.java110.cms.dao.StatisticDao">

    <resultMap type="statistic" id="stscMap">
        <id column="mno" property="mno" />
        <result column="grno" property="grno" />
        <result column="gr_nm" property="gr_nm" />
    </resultMap>


  <select id="stscLabel" resultMap="stscMap" parameterType="int">
   select m.mno, m.grno, g.gr_nm
   from mv_memb_gr m
   INNER JOIN mv_gr g on m.grno =g.grno
   where m.mno =#{mno}
   order by m.grno
  
  </select>

  

  
</mapper>
